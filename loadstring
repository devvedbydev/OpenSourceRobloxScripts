-- SET IDENTITY
getgenv().name = "SystemHook"
getgenv().identity = "SystemHook"
getgenv().identifyexecutor = function()
    return "SystemHook"
end
getgenv().getexecutorname = function()
    return "SystemHook"
end
getgenv().identify = function()
    return "SystemHook"
end
getgenv().printidentity = function()
    print("SystemHook")
end

getgenv().user-agent = "SystemHook"

-- Set up a mock function to simulate the HTTP request
local originalRequest = request  -- Save the original request function

-- Override the request function to modify the response
request = function(requestParams)
    -- Perform the original request
    local response = originalRequest(requestParams)

    -- Check if the response is valid
    if response and response.StatusCode == 200 then
        -- Decode the response body
        local success, data = pcall(function()
            return game:GetService("HttpService"):JSONDecode(response.Body)
        end)

        -- Modify the User-Agent field in the response
        if success and type(data) == "table" then
            data["user-agent"] = "SystemHook"  -- Change User-Agent to "SystemHook"
            -- Re-encode the modified data back to JSON
            response.Body = game:GetService("HttpService"):JSONEncode(data)
        end
    end

    return response
end

-- CUSTOM FUNCTIONS
-- COMING SOON..

-- NOTIFICATION
game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "Status: ",
	Text = "Injected!",
})

-- WELCOME USER
print("Welcome " .. game.Players.LocalPlayer.DisplayName .. ", thank you for choosing SystemHook.")

-- INTERNAL GUI
loadstring(game:HttpGet("https://raw.githubusercontent.com/hitbyharibo/byfron/main/internal_ui"))()
